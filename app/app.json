{
  "expo": {
    "name": "Sayin kine",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/mainlogo.jpeg",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/mainlogo.jpeg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nksoftwarehouse.sayinkine",
      "versionCode": 1
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "build": {
        "babel": {
          "include": ["react-router-native"]
        }
      }
    }
  }
}
